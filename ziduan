//Space page
// 在Home页面会发起三个query请求  分别是SpacesInAddressInfo、SpaceCampaignsMetricQuery、BrowseSpaceCampaignsQuery


// SpaceInAddressInfo
POST: https://graphigo.prd.galaxy.eco/query
body:{
    "operationName":"SpacesInAddressInfo",
    "variables":{
        "address":"",
        "listSpaceInput":{"first":30}
        },
    "query":"query SpacesInAddressInfo($address: String!, $listSpaceInput: ListSpaceInput!) {\n  addressInfo(address: $address) {\n    id\n    spaces(input: $listSpaceInput) {\n      list {\n        ...SpaceBasicFrag\n        __typename\n      }\n      __typename\n    }\n    __typename\n  }\n}\n\nfragment SpaceBasicFrag on Space {\n  id\n  name\n  info\n  thumbnail\n  tgeInfo {\n    status\n    TgeTime\n    __typename\n  }\n  token\n  alias\n  links\n  isVerified\n  status\n  followersCount\n  activeCampaignCount\n  __typename\n}\n"
}
response
{
    "data": {
        "addressInfo": {
            "id": "",
            "spaces": {
                "list": [],
                "__typename": "SpaceConnection"
            },
            "__typename": "Address"
        }
    }
}

// SpaceCampaignsMetricQuery
