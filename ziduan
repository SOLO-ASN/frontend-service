// Space page Leaderboard
// 在Leaderboard上有两个query请求，分别是SpaceLeaderboard、SeasonList

// SeasonList
POST: https://graphigo.prd.galaxy.eco/query
body:{
    "operationName":"SeasonList",
    "variables":{
        "spaceId":"994",
        "first":20,
        "after":"-1",
        "isAdminView":false
        },
    "query":"query SeasonList($spaceId: Int!, $first: Int!, $after: String!, $isAdminView: Boolean!) {\n  space(id: $spaceId) {\n    id\n    pointsLeaderboardVisible\n    sprints(first: $first, after: $after, isAdminView: $isAdminView) {\n      list {\n        ...SeasonFrag\n        __typename\n      }\n      totalCount\n      pageInfo {\n        startCursor\n        endCursor\n        hasNextPage\n        __typename\n      }\n      __typename\n    }\n    __typename\n  }\n}\n\nfragment SeasonFrag on Sprint {\n  id\n  name\n  space {\n    id\n    __typename\n  }\n  startTime\n  endTime\n  isVisible\n  __typename\n}\n"
}

